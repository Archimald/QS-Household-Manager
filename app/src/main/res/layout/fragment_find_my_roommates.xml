        android:orientation="vertical">

        <ListView
            android:id="@+id/members_list_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">


        </ListView>

    </android.support.v7.widget.LinearLayoutCompat>

</FrameLayout>
